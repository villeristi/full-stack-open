  "scripts": {
    "ui:start": "node scripts/start.js",
    "api:start": "node ./api/index",
    "api:dev": "nodemon --watch api ./api/index",
    "ui:build": "node scripts/build.js",
    "lint": "eslint .",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
