{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "no-console": "off",
    "semi": [2, "never"],
    "comma-dangle": [2, "always-multiline"],
    "eol-last": [2, "always"],
    "indent": [2, 2, {"SwitchCase":1}],
    "no-irregular-whitespace": "off"
  }
}
